{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended"],
  "enabledManagers": ["maven", "maven-wrapper"],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "major",
        "minor",
        "patch",
        "digest",
        "pin"
      ],
      "automerge": true,
      "platformAutomerge": true
    }
  ]
}
